{
    "name": "gordalina/cachetool",
    "description": "Manage your OPcache & APCu cache through the CLI",
    "keywords": [
        "cache",
        "opcache",
        "apcu",
        "fastcgi",
        "opcode",
        "fpm"
    ],
    "require": {
        "php": ">=8.2.0",
        "ext-dom": "*",
        "ext-xml": "*",
        "hollodotme/fast-cgi-client": "^3.0",
        "monolog/monolog": "^3.0",
        "psr/log": "^2.0 || ^3.0",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/finder": "^7.0",
        "symfony/process": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/http-foundation": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.19"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "gordalina",
            "email": "samuel.gordalina@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CacheTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CacheTool\\": "tests/"
        }
    },
    "bin": [
        "bin/cachetool"
    ]
}
